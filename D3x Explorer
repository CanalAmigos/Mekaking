    spawn(function()
        Settings['Save'] = {}

        Save:Textbox('Config Name', '', false, function(v)
            Settings['Save'].Name = v
        end)

        local Configs = Save:Dropdown('Configs', {}, function(v)
            Settings['Save'].Load = v
        end)

        local function RefreshList()
            if type(listfiles) == 'function' then
                local list = listfiles('canalamigos/configs/DrangonBloxX')
                local out = {}
                for i = 1, #list do
                    local file = list[i]
                    if file:sub(-5) == '.json' then
                        local pos = file:find('.json', 1, true)
                        local start = pos
                        local char = file:sub(pos, pos)
                        while char ~= '/' and char ~= '\\' and char ~= '' do
                            pos = pos - 1
                            char = file:sub(pos, pos)
                        end
                        if char == '/' or char == '\\' then
                            table.insert(out, file:sub(pos + 1, start - 1))
                        end
                    end
                end
                Configs:Refresh(out)
            end
        end
        pcall(RefreshList)

        Save:Button('Create', function()
            if type(makefolder) == 'function' then makefolder('canalamigos/configs/DrangonBloxX')
                local name = Settings['Save'].Name or ''
                if name:gsub(' ','') == '' then
                    return library:Notification('Save','Invalid config name','Okay!')
                else
                    name = name:gsub(' ','_')
                end
                local save = {}
                for i,v in pairs({'Beams','Charge','Rebirth','Strenght','Speed','Ki','GodMode','Farm','LeftRight','FarmPoint','Esp','EspColor'}) do
                    save[v] = Settings[v]
                end
                if gui:Save(save,name,'canalamigos/configs/DrangonBloxX') == false then
                    return library:Notification('Save','Your exploit don\'t support this function!',':(',10)
                else
                    pcall(RefreshList)
                end
            end
        end)

        Save:Button('Save', function()
            if type(makefolder) == 'function' and type(isfile) == 'function' then makefolder('canalamigos/configs/DrangonBloxX')
                local name = Settings['Save'].Load or ''
                if not isfile('canalamigos/configs/DrangonBloxX/'..name..'.json') then return end
                local save = {}
                for i,v in pairs({'Beams','Charge','Rebirth','Strenght','Speed','Ki','GodMode','Farm','LeftRight','FarmPoint','Esp','EspColor'}) do
                    save[v] = Settings[v]
                end
                if gui:Save(save,name,'canalamigos/configs/DrangonBloxX') == false then
                    return library:Notification('Save','Your exploit don\'t support this function!',':(',10)
                else
                    pcall(RefreshList)
                end
            end
        end)

        Save:Button('Delete', function()
            if type(isfile) == 'function' and type(delfile) == 'function' then
                local file = 'canalamigos/configs/DrangonBloxX/'..Settings.Save.Load..'.json'
                if not isfile(file) then return library:Notification('Save','Config '..Settings.Save.Load..' does not exist','Okay!') end
                local s,e = pcall(delfile,file)
                if not s then
                    library:Notification('Delete','error occured during file deletion: '..e,'Okay!')
                else
                    pcall(RefreshList)
                end
            else
                library:Notification('Save','Your exploit don\'t support this function!','Okay!',10)
            end
        end)

        Save:Button('Load', function()
            if string.gsub(Settings.Save.Load,' ','') ~= '' then
                local Load = {}
                if gui:LoadSave(Load,Settings.Save.Load,'canalamigos/configs/DrangonBloxX') then
                    for i,v in pairs(Load) do
                        if typeof(v) == 'boolean' then
                            pcall(function() LoadSettings[i]:SetToggle(v) end)
                        elseif typeof(v) == 'Color3' then
                            pcall(function() LoadSettings[i]:SetColor(v) end)
                        elseif typeof(v) == 'number' then
                            pcall(function() LoadSettings[i]:Change(v) end)
                        elseif typeof(v) == 'string' then
                            pcall(function() LoadSettings[i]:SetText(v) end)
                        elseif typeof(v) == 'table' then
                            for i_,v in pairs(v) do
                                if typeof(v) == 'boolean' then
                                    pcall(function() LoadSettings[i][i_]:SetToggle(v) end)
                                elseif typeof(v) == 'Color3' then
                                    pcall(function() LoadSettings[i][i_]:SetColor(v) end)
                                elseif typeof(v) == 'number' then
                                    pcall(function() LoadSettings[i][i_]:Change(v) end)
                                elseif typeof(v) == 'string' then
                                    pcall(function() LoadSettings[i][i_]:SetText(v) end)
                                elseif typeof(v) == 'table' then
                                    for _i,v in pairs(v) do
                                        if typeof(v) == 'boolean' then
                                            pcall(function() LoadSettings[i][i_][_i]:SetToggle(v) end)
                                        elseif typeof(v) == 'Color3' then
                                            pcall(function() LoadSettings[i][i_][_i]:SetColor(v) end)
                                        elseif typeof(v) == 'number' then
                                            pcall(function() LoadSettings[i][i_][_i]:Change(v) end)
                                        elseif typeof(v) == 'string' then
                                            pcall(function() LoadSettings[i][i_][_i]:SetText(v) end)
                                        end
                                        Settings[i][i_][_i] = v
                                    end
                                    continue
                                end
                                Settings[i][i_] = v
                            end
                            continue
                        end
                        Settings[i] = v
                    end
                    library:Notification('Load','Loaded '..Settings.Save.Load..' Successfully!','Okay!')
                else
                    library:Notification('Load','Failed to load '..Settings.Save.Load..'!','Okay!')
                end
            end
        end)

        Save:Button('Refresh', function()
            if type(listfiles) == 'function' then
                pcall(RefreshList)
            else
                library:Notification('Refresh','Your exploit don\'t support this function!','Okay!')
            end
        end)
    end)

