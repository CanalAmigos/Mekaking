    function CreateSong(id,volume)
        spawn(function()
            local song = Instance.new('Sound')
            song.SoundId = 'rbxassetid://'..id or 'nil'
            song.Volume = volume or 0.5
            song.Playing = true
            song.PlayOnRemove = true
            ds:AddItem(song,0)
        end)
        return
    end

    Keys:Bind('Manual Farm',Enum.KeyCode.Minus,false,function()
        Manualfarm = not Manualfarm
