    local function rstring(t)
        return t[math.random(1, #t)]
    end

    function checkUnequip(toolname)
        for _, tool in ipairs(pl.Character:GetChildren()) do
            if tool:IsA('Tool') and tool.Name == toolname then
                tool.Parent = pl.Backpack
                return
            end
        end
    end
    function checkBackpack(toolname)
        for _, tool in ipairs(pl.Backpack:GetChildren()) do
            if tool:IsA('Tool') and tool.Name == toolname then
                tool.Parent = pl.Character
                return
            end
        end
        game:GetService('ReplicatedStorage').Events.giveskill:FireServer(toolname)
        if not pl.Character:FindFirstChild(toolname) and not pl.Backpack:FindFirstChild(toolname) then
            return checkBackpack()
        end
        return
    end

    local Animations = {
