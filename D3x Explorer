        Atack = not Atack
        Manualfarm = false
    end)
    local MAC = false
    Keys:Bind('Manual Atack',Enum.KeyCode.Z,false,function()
        if not MAC then
            MAC = true
            checkBackpack('Energy Wave')
            pcall(function()
                if pl.Character:FindFirstChild('Energy Wave') then
                    local LoadAnim = pl.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Animations['Energy Wave'])
                    LoadAnim:Play()
                    LoadAnim:AdjustSpeed(1.4)
                    local Event = Events.Beam
                    local beamsize = Settings.Beams.Atack
                    local maincolor = rstring(colors)
                    local A_1 = mouse.Hit.p
                    local A_2 = pl.Character['Energy Wave']
                    Event:FireServer(false, 'EnergyWave', maincolor, 3, A_1, 3.2, 50, A_2, 0, 50)
                    rs.RenderStepped:Wait()
                    local A_1 = mouse.Hit.p
                    local A_2 = pl.Character['Energy Wave']
                    Event:FireServer(true, 'EnergyWave', maincolor, 1, A_1, math.huge, 0, A_2, beamsize, 1, 0, 1)
                end
            end)
            checkUnequip('Energy Wave')
            wait(0.7)
            MAC = false
        end
    end)
    Keys:Bind('Change The Atack Mode {Kill,Health}',Enum.KeyCode.LeftCurly,false,function()
