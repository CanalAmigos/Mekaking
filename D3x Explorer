    Main:Seperator()

    local plrs = {}
    for i,v in pairs(game.Players:GetPlayers()) do
        if v:IsA('Player') and v ~= pl and not table.find(plrs, v.Name) then 
            table.insert(plrs, v.Name)
            table.sort(plrs,function(a,b) return a:lower() < b:lower() end)
        end;
    end;

    local players = Main:Dropdown('Player',plrs,function(t)
        Settings['Player'] = t
    end)

    game.Players.PlayerAdded:Connect(function(player)
        if player:IsA('Player') then 
            table.insert(plrs, player.Name)
            table.sort(plrs,function(a,b) return a:lower() < b:lower() end)
            players:Refresh(plrs)
        end;
    end);

    game.Players.PlayerRemoving:Connect(function(player)
        if player:IsA('Player') then 
            for i,v in pairs(plrs) do 
                if v == player.Name then 
                    table.remove(plrs, i)
                    table.sort(plrs,function(a,b) return a:lower() < b:lower() end)
                end;
            end;
        end;
        players:Refresh(plrs)
    end);

    Main:Button('Kill', function()
        if Killing == false then
            library:Notification('Player','Trying to kill player!','Okay!')
            rs.RenderStepped:Wait()
            Killing = true
            lastpoint = pl.Character.PrimaryPart.CFrame
            pcall(function()
                if pls:FindFirstChild(Settings['Player']) ~= nil and pls:FindFirstChild(Settings['Player']) ~= pl then
                    Beckup['Farm'] = Settings['Farm']
                    Beckup['Manualfarm'] = Manualfarm
                    Beckup['Atack'] = Atack
                    Settings['KillAborted'] = false
                    local targetplayer = pls:WaitForChild(Settings['Player'])
                    local hum = targetplayer.Character:WaitForChild('HumanoidRootPart')
                    spawn(function()
                        spawn(function()
                            while Killing and dead == false do
                                rs.RenderStepped:Wait()
                                Settings['Farm'] = false
                                Manualfarm = false
                                Atack = false
                            end
                        end)
                        while Killing and dead == false do
                            rs.RenderStepped:Wait()
                            pcall(function()
                                pl.Character.PrimaryPart.CFrame = hum.CFrame + Vector3.new(0,9,0)
                            end)
                        end
                    end)
                    local leave = pls.PlayerRemoving:Connect(function(v)
                        if v == targetplayer and Killing then
                            Killing = false
                            targetplayer = 'leave'
                            library:Notification('Player','Fail to kill player [Target Leaved]','Okay!')
                        end
                    end)
                    targetplayer.Character:WaitForChild('Humanoid').Died:Connect(function()
                        Killing = false
                    end)
                    local Event = Events.Beam
                    while Killing and not dead do
                        rs.RenderStepped:Wait()
                        local pos = Vector3.new(hum.Position.X,hum.Position.Y,hum.Position.Z)
                        checkBackpack('Energy Wave')
                        pcall(function()
                            wait(0.62)
                            pcall(function()
                                local A_1 = pos
                                local A_2 = pl.Character['Energy Wave']
                                Event:FireServer(false, 'EnergyWave', 'Red', 3, A_1,--[[damage]]math.huge, 20, A_2, 17, 8)
                                rs.RenderStepped:Wait()
                                local A_1 = pos
                                local A_2 = pl.Character['Energy Wave']
                                Event:FireServer(true, 'EnergyWave', 'Red', 3, A_1,--[[damage]]math.huge, 20, A_2, 17, 8, 30, 1)
                            end)
                        end)
                    end
                    checkUnequip('Energy Wave')
                    if Killing == false and dead == false and targetplayer ~= 'leave' and Settings['KillAborted'] == false then
                        library:Notification('Player','Killed player','Okay!')
                        CreateSong(3450794184,2)
                        pl.Character.PrimaryPart.CFrame = lastpoint
                    elseif Settings['KillAborted'] then
                        pl.Character.PrimaryPart.CFrame = lastpoint
                    elseif dead then
                        library:Notification('Player','Fail you died','Okay!')
                        CreateSong(2865228021,.7)
                        Killing = false
                    end
                    lastpoint = nil
                    Killing = false
                    leave:Disconnect()
                    Settings['Farm'] = Beckup['Farm']
                    Manualfarm = Beckup['Manualfarm']
                    Atack = Beckup['Atack']
                else
                    library:Notification('Player','Fail player not found','Okay!')
                    CreateSong(2865228021,.7)
                    Killing = false
                end
                return
            end)
            Killing = false;
        end
    end)

    Main:Button('Abort', function()
        if Killing then
            Killing = false
            Settings['KillAborted'] = true
            library:Notification('Player','Aborting...','Okay!')
        end
    end)

