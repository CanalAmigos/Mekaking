    Keys:Bind('Move The Dummies',Enum.KeyCode.K,false,function()
        if Settings['MoveType'] == 'All' then
            for i,v in ipairs(game.Workspace:GetChildren()) do
                pcall(function()
                    if string.find(string.lower(v.Name), 'dummy', 1, true) then
                        local hum = v:FindFirstChildOfClass('Humanoid')
                        if (pl.Character.PrimaryPart.Position - v.HumanoidRootPart.Position).Magnitude <= 2000 and hum and tostring(hum.Health) ~= 'nan' then
                            local args = {
                                [1] = 'heavy',
                                [2] = -math.huge,
                                [3] = v.Humanoid,
                                [4] = v.HumanoidRootPart,
                                [5] = Settings["NoEffect"],
                                [6] = v.Head,
                                [7] = 0,
                                [8] = 'explosion'
                            }
                            Events.Dmg:FireServer(unpack(args))
                        elseif hum and tostring(hum.Health) == 'nan' and not v:FindFirstChild("Buged") then
                            Create("Highlight",{Name = 'Buged',Parent = v,FillTransparency = 1,OutlineColor = Color3.new(1, 0, 0),DepthMode = Enum.HighlightDepthMode.Occluded})
                        end
                    end
                end)
            end
        elseif Settings['MoveType'] == 'Line' then
            pcall(function()
                local Dummyes = {}
                for i,v in ipairs(game.Workspace:GetChildren()) do
                    pcall(function()
                        if string.find(string.lower(v.Name), 'dummy', 1, true) then
                            local hum = v:FindFirstChildOfClass('Humanoid')
                            if (pl.Character.PrimaryPart.Position - v.HumanoidRootPart.Position).Magnitude <= 2000 and hum and tostring(hum.Health) ~= 'nan' then
                                Dummyes[#Dummyes+1] = v
                            elseif hum and tostring(hum.Health) == 'nan' and not v:FindFirstChild("Buged") then
                                Create("Highlight",{Name = 'Buged',Parent = v,FillTransparency = 1,OutlineColor = Color3.new(1, 0, 0),DepthMode = Enum.HighlightDepthMode.Occluded})
                            end
                        end
                    end)
                end
                for i,v in pairs(Dummyes) do
                    pcall(function()
                        if i > 1 then
                            wait(0.1)
                            pl.Character:PivotTo(pl.Character:GetPivot() * CFrame.new(0, 0, -1.6))
                        end
                        local args = {
                            [1] = 'heavy',
                            [2] = -math.huge,
                            [3] = v.Humanoid,
                            [4] = v.HumanoidRootPart,
                            [5] = Settings["NoEffect"],
                            [6] = v.Head,
                            [7] = 0,
                            [8] = 'explosion'
                        }
                        Events.Dmg:FireServer(unpack(args))
                    end)
                end
            end)
        end
    end)
    Keys:Bind('Change Dummies Move Type',Enum.KeyCode.M,false,function()
        if Settings['MoveType'] == 'All' then
            Settings['MoveType'] = 'Line'
            library:Notification('Line','Move Dummies type is now Line','Okay!')
        else
            Settings['MoveType'] = 'All'
            library:Notification('All','Move Dummies type is now All','Okay!')
        end
    end)
    Keys:Bind('Crashes The Server',Enum.KeyCode.N,false,function()
        checkBackpack('Energy Wave')
        pcall(function()
            local maincolor = rstring(colors)
            local A_1 = Vector3.new(90, 0, 0)
            local A_2 = pl.Character['Energy Wave']
            Events.Beam:FireServer(false, 'EnergyWave', maincolor, 5, A_1, 3.2, 50, A_2, -99999999, 50)
            rs.RenderStepped:Wait()
            local A_1 = Vector3.new(90, 0, 0)
            local A_2 = pl.Character['Energy Wave']
            Events.Beam:FireServer(true, 'EnergyWave', maincolor, 1, A_1, 0, 0, A_2, -99999999, 0, 0, 25)
        end)
    end)
    Keys:Bind('Give Inf Health To Players Proximit to you',Enum.KeyCode.U,false,function()
        for i,v in pairs(pls:GetPlayers()) do
            pcall(function()
                if v:IsA('Player') and (pl.Character.PrimaryPart.Position - v.Character.PrimaryPart.Position).Magnitude <= 10 then
                    if v.Name ~= pl.Name then
                        local args = {
                            [1] = 'Knockback',
                            [2] = -math.huge,
                            [3] = v.Character:FindFirstChildOfClass("Humanoid"),
                            [4] = v.Character.PrimaryPart,
                            [5] = Settings["NoEffect"],
                            [6] = pl.Character.RightHand,
                            [7] = 0
                        }
                        Events.Dmg:FireServer(unpack(args))
                    end
                end
            end)
        end
    end)
    Keys:Bind('Fire 15 Lazers Atack',Enum.KeyCode.V,false,function()
