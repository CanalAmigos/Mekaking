    spawn(function()
        local data = game:GetService('ReplicatedStorage').Datas
        while true do rs.RenderStepped:Wait()
            pcall(function()
                if Settings.Esp then
                    for i,v in pairs(pls:GetPlayers()) do
                        if v:IsA('Player') and not v.Character.Head:FindFirstChild('DysplayThings') then
                            local Rebirthh,Health
                            if data:FindFirstChild(v.Name) then Rebirthh = data:FindFirstChild(v.Name).Rebirth.Value else Rebirthh = 'nil' end
                            if v.Character:FindFirstChildOfClass('Humanoid') then Health = v.Character:FindFirstChildOfClass('Humanoid').Health else Health = 'nil' end
                            if Health ~= 'nil' then Health = string.split(Health,'.') if type(Health) == 'table' then Health = Health[1] end end
                            local DysplayThings = Instance.new('BillboardGui')
                            local Rhings = Instance.new('TextLabel')
                            local Hight = Instance.new('Highlight')

                            DysplayThings.Name = 'DysplayThings'
                            DysplayThings.Parent = v.Character.Head
                            DysplayThings.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                            DysplayThings.Active = true
                            DysplayThings.AlwaysOnTop = true
                            DysplayThings.ExtentsOffset = Vector3.new(0, 4, 0)
                            DysplayThings.Size = UDim2.new(0, 400, 0, 50)

                            Rhings.Name = 'Rhings'
                            Rhings.Parent = DysplayThings
                            Rhings.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                            Rhings.BackgroundTransparency = 1.000
                            Rhings.Size = UDim2.new(1, 0, 1, 0)
                            Rhings.Font = Enum.Font.SourceSans
                            Rhings.TextStrokeTransparency = 0
                            Rhings.TextScaled = true
                            Rhings.TextWrapped = true
                            Rhings.Text = '[Name: '..v.Name..']\n[Health: '..Health..']\n[Rebirths: '..Rebirthh..']'
                            Rhings.TextColor3 = Settings['EspColor']
                            Rhings.TextSize = 14

                            if v ~= pl then
                                Hight.Name = 'HightEsp'
                                Hight.Parent = DysplayThings
                                Hight.Adornee = v.Character
                                Hight.FillColor = Color3.new(1, 1, 1)
                                Hight.FillTransparency = 1
                                Hight.OutlineColor = Settings['EspColor']
                                Hight.OutlineTransparency = 1
                            end
                            spawn(function()
                                while true do rs.RenderStepped:Wait()
                                    if DysplayThings == nil then break end
                                    pcall(function()
                                        local Rebirthh,Health
                                        if data:FindFirstChild(v.Name) then Rebirthh = data:FindFirstChild(v.Name).Rebirth.Value else Rebirthh = 'nil' end
                                        if v.Character:FindFirstChildOfClass('Humanoid') then Health = v.Character:FindFirstChildOfClass('Humanoid').Health else Health = 'nil' end
                                        if Health ~= 'nil' then Health = string.split(Health,'.') if type(Health) == 'table' then Health = Health[1] end end
                                        Rhings.Text = '[Name: '..v.Name..']\n[Health: '..Health..']\n[Rebirths: '..Rebirthh..']'
                                        Rhings.TextColor3 = Settings['EspColor']
                                    end)
                                end
                            end)
                        end
                    end
                else
                    for i,v in pairs(pls:GetPlayers()) do
                        if v:IsA('Player') and v.Character.Head:FindFirstChild('DysplayThings') then
                            pcall(function()
                                ds:AddItem(v.Character.Head:WaitForChild('DysplayThings'),0)
                            end)
                        end
                    end
                end
            end)
            if MainBreak then break end
        end
    end)

