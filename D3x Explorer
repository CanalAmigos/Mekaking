    Skills:Button('Add',function()
        rs.RenderStepped:Wait()
        if Settings['AutoSkills'].Type == 'Custom' and not table.find(Settings['AutoSkills'].Value,Settings['AutoSkills'].Add) then
            Settings['AutoSkills'].Value[#Settings['AutoSkills'].Value+1] = Settings['AutoSkills'].Add
            Settings['AutoSkills'].Custom = Settings['AutoSkills'].Value
        end
        Skillvalue:Refresh(Settings['AutoSkills'].Custom)
    end)

    Skills:Button('Remove',function()
        if Settings['AutoSkills'].Type == 'Custom' and table.find(Settings['AutoSkills'].Value,Settings['AutoSkills'].Remove) then
            local ToRemove = 0
            for i = 1,#Settings['AutoSkills'].Value do
                if Settings['AutoSkills'].Value[i] == Settings['AutoSkills'].Remove then
                    ToRemove = i
                end
            end
            table.remove(Settings['AutoSkills'].Value,ToRemove)
            Settings['AutoSkills'].Custom = Settings['AutoSkills'].Value
            Skillvalue:Refresh(Settings['AutoSkills'].Custom)
        end
    end)

