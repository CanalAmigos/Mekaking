                        local A_5 = farmcframe
                        local A_8 = pl.Character['Kamehameha']
                        Event:FireServer(true, 'Kamehameha', 'Blue',4, A_5,--[[damage]]0, 35, A_8, beamsize, 9, 50, 0.5)
                    end)
                    wait(0.7)
                    --[[
                    pcall(function()
                        LoadAnim = pl.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Animations['Galick Gun'])
                        LoadAnim:Play()
                        LoadAnim:AdjustSpeed(0.755)
                        local A_5 = farmcframe
                        local A_8 = pl.Character['Galick Gun']
                        Event:FireServer(false, 'GalickGun', 'Purple', 4, A_5,--[[damage]0, 35, A_8, beamsize, 9)
                        rs.RenderStepped:Wait()
                        local A_5 = farmcframe
                        local A_8 = pl.Character['Galick Gun']
                        local Event = game:GetService('ReplicatedStorage').Events.Beam
                        Event:FireServer(true, 'GalickGun', 'Purple', 4, A_5,--[[damage]0, 35, A_8, beamsize, 9, 50, 0.5)
                        LoadAnim:Stop(0)
                    end)
                    wait(0.66)]]
                    spawn(function()
                        --Auto save the game while farming
                        if not dead and not rebirthing then
                            SaveN += 1
                            if SaveN == 3 then
                                Save:FireServer()
                                SaveN = 0
                            end
                        end
                    end)
                elseif Atack and not Health and not Manualfarm and not Settings.Farm and lastpoint == nil and not dead and not rebirthing then
                    local beamsize = Settings.Beams.Atack
                    wait(0.66)
                    pcall(function()
                        local A_1 = farmcframe
                        local A_2 = pl.Character['Energy Wave']
                        Event:FireServer(false, 'EnergyWave', 'Red', 3, A_1,--[[damage]]math.huge, 20, A_2, beamsize, 8)
                        rs.RenderStepped:Wait()
                        local A_1 = farmcframe
                        local A_2 = pl.Character['Energy Wave']
                        Event:FireServer(true, 'EnergyWave', 'Red', 3, A_1,--[[damage]]math.huge, 20, A_2, beamsize, 8, 30, 1)
                    end)
                elseif Health and Atack and not Manualfarm and not Settings.Farm and lastpoint == nil and not dead and not rebirthing then
                    local beamsize = Settings.Beams.Health
                    wait(0.66)
                    pcall(function()
                        local A_1 = farmcframe
                        local A_2 = pl.Character['Energy Wave']
                        Event:FireServer(false, 'EnergyWave', 'Green', 3, A_1,--[[damage]]-math.huge, 20, A_2, beamsize, 8)
                        rs.RenderStepped:Wait()
                        local A_1 = farmcframe
                        local A_2 = pl.Character['Energy Wave']
                        Event:FireServer(true, 'EnergyWave', 'Green', 3, A_1,--[[damage]]-math.huge, 20, A_2, beamsize, 8, 30, 1)
                    end)
                end
            end)
            if MainBreak then break end
        end
    end)

    function Respawn()
        ds:AddItem(pl.PlayerGui:WaitForChild('LocalScript'),0)
        rs.RenderStepped:Wait()
        Events.Rs:InvokeServer()
        rs.RenderStepped:Wait()
        wait(2)
        pcall(function() ds:AddItem(pl.PlayerGui.Start,0) end)
        pl.Character:WaitForChild('HumanoidRootPart').CFrame = lastpoint
        workspace.CurrentCamera.CameraSubject = pl.Character
        workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        workspace.CurrentCamera.CFrame = Camerapos
        game:GetService('StarterGui'):SetCoreGuiEnabled(Enum.CoreGuiType.All,true)
        pcall(function() game.Lighting:FindFirstChildOfClass('BlurEffect').Enabled = false end)
        pcall(function() pl.PlayerGui:FindFirstChild('ScreenGui').Enabled = true end)
        AutoSkill()
        lastpoint = nil
        Camerapos = nil
        rebirthing = false
        dead = false
        return
    end

    spawn(function()
        local cam = workspace.CurrentCamera
        local rki = Events.Blast
        while true do
            rs.RenderStepped:Wait()
            pcall(function()
                if Settings['Rebirth'] then
                    local Check = pl.PlayerGui:WaitForChild('ScreenGui'):WaitForChild('Full'):WaitForChild('StatFrame')
                    local Speed = Check:WaitForChild('Speed')
                    local Strength = Check:WaitForChild('Strength')
                    local Ki = Check:WaitForChild('Ki')
                    local Stats1 = Speed.Text:gsub('%D+', '')
                    local Stats2 = Strength.Text:gsub('%D+', '')
                    local Stats3 = Ki.Text:gsub('%D+', '')
                    if tonumber(Stats1) >= 500 and tonumber(Stats2) >= 500 and tonumber(Stats3) >= 500 then
                        if rebirthing == false then
                            rebirthing = true
                            lastpoint = pl.Character:WaitForChild('HumanoidRootPart').CFrame
                            Camerapos = workspace.CurrentCamera.CFrame

                            local Sucess,Error = pcall(function()
                                game:GetService('ReplicatedStorage').Events.re:InvokeServer()
                                rebirthed += 1
                            end)
                            pl.CharacterAdded:Wait()
                            wait(0.2)
                            if Sucess then
                                library:Notification('Rebirth','Sucefully rebirthed! ['..rebirthed..']','Okay!')
                                CreateSong(3450794184,2)
                            else
                                library:Notification('Rebirth',Error,'Okay!')
                                CreateSong(2865228021,.7)
                            end
                            if not Settings['Farm'] and not Manualfarm and not Atack then
                                pl.PlayerGui:WaitForChild('Start'):WaitForChild('Full'):WaitForChild('start').MouseButton1Click:Wait()
                                wait(2)
                                pl.Character:WaitForChild('HumanoidRootPart').CFrame = lastpoint
                                pcall(AutoSkill)
                                lastpoint = nil
                                rebirthing = false
                            else
                                wait(5)
                                pcall(Respawn)
                            end
                        end
                    elseif tonumber(Stats3) < 5 and not dead and not rebirthing then
                        pcall(function()
                            local A_1 = pl.Character:WaitForChild('Effects')
                            local A_2 = Vector3.new(0, 0, 0)
                            local A_3 = pl.Character:WaitForChild('RightHand')
                            rki:FireServer(true, A_1, A_2, A_3)
                        end)
                    end
                end
            end)
            if MainBreak then break end
        end
    end)

    local aurapillars = false
    pl.Chatted:Connect(function(chat)
        if MainBreak then return end
        chat = chat:lower():gsub('/e ','')
        if string.sub(chat, 0, 8) == 'kaioblue' or string.sub(chat, 0, 9) == 'superkaio' then
            Events.TF:FireServer('kaioblue')
        elseif string.sub(chat, 0, 7) == 'pillars' then
            if string.split(chat, ' ')[2] == 'true' then
                aurapillars = true
                while aurapillars == true do
                    local args = {
                        [1] = 'Pillars',
                        [2] = pl.Character.PrimaryPart,
                        [3] = 'Blue',
                        [4] = 0.30,
                        [5] = 6,
                        [6] = Vector3.new(.5,3,.5),
                        [7] = Vector3.new(.5,3,.5),
                        [8] = 0,
                        [9] = pl.Character.Effects
                    }
                    Events.Effect:FireServer(unpack(args))
                    local args = {
                        [1] = 'Pillars',
                        [2] = pl.Character.PrimaryPart,
                        [3] = 'LightBlue',
                        [4] = 0.30,
                        [5] = 6,
                        [6] = Vector3.new(.5,3,.5),
                        [7] = Vector3.new(.5,3,.5),
                        [8] = 0,
                        [9] = pl.Character.Effects
                    }
                    Events.Effect:FireServer(unpack(args))
                    local args = {
                        [1] = 'Pillars',
                        [2] = pl.Character.PrimaryPart,
                        [3] = 'White',
                        [4] = 0.30,
                        [5] = 6,
                        [6] = Vector3.new(.5,3,.5),
                        [7] = Vector3.new(.5,3,.5),
                        [8] = 0,
                        [9] = pl.Character.Effects
                    }
                    Events.Effect:FireServer(unpack(args))
                    wait(1.5)
                end
            elseif string.split(chat, ' ')[2] == 'false' then
                aurapillars = false
            end
        end
    end)

    spawn(function()
        while true do rs.RenderStepped:Wait()
            if MainBreak then break end
            if lastpoint == nil and rebirthing == false then
                pl.Character:FindFirstChildOfClass('Humanoid').Died:Wait()
                if MainBreak then break end
                if lastpoint == nil and rebirthing == false then
                    dead = true
                    lastpoint = pl.Character:WaitForChild('HumanoidRootPart').CFrame
                    Camerapos = workspace.CurrentCamera.CFrame
                    pl.CharacterAdded:Wait()
                    wait(0.2)
                    if not Settings['Farm'] and not Manualfarm and not Atack then
                        pl.PlayerGui:WaitForChild('Start'):WaitForChild('Full'):WaitForChild('start').MouseButton1Click:Wait()
                        wait(2)
                        pl.Character:WaitForChild('HumanoidRootPart').CFrame = lastpoint
                        pcall(AutoSkill)
                        lastpoint = nil
                        dead = false
                    elseif rebirthing == false then
                        wait(5)
                        pcall(Respawn)
                    end
                end
            end
        end
    end)
end
