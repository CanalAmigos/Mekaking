    function AutoSkill()
        spawn(function()
            for i,v in pairs(Settings['AutoSkills'].Value) do
                if string.gsub(v,' ','') ~= '' then
                    local Sucess,Error = pcall(function()
                        game:GetService('ReplicatedStorage').Events.giveskill:FireServer(v)
                    end)
                    if Sucess == false then
                        library:Notification('AutoSkill',Error,'Okay!')
                        CreateSong(2865228021,.7)
                    end
                end
            end
        end)
        return
    end

